{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "ngx-file-drop"
  },
  "apps": [
    {
      "root": "lib",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "sandbox/index.html",
      "main": "sandbox/main.ts",
      "polyfills": "sandbox/polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "demo",
      "styles": [
        "sandbox/styles.scss"
      ],
      "scripts": [],
      "environmentSource": "sandbox/environments/environment.ts",
      "environments": {
        "dev": "sandbox/environments/environment.ts",
        "prod": "sandbox/environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "lib/tsconfig.app.json"
    },
    {
      "project": "lib/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {}
  }
}
